{
  "name": "@serabass/lrx",
  "version": "1.0.0",
  "description": "Learning how to create React modules using TypeScript!",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "npm run build:esm && npm run build:cjs",
    "build:watch": "npm run build:esm --watch && npm run build:cjs --watch ",
    "build:esm": "tsc",
    "build:peg": "npx pegjs src/parser.pegjs",
    "build:peg:watch": "npx nodemon -w ./ -e pegjs -x pegjs src/parser.pegjs",
    "build:cjs": "npx tsc --module commonjs --outDir lib/cjs",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src/**/*.{ts,tsx}"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.109",
    "@types/jest": "^24.9.0",
    "@types/pegjs": "^0.10.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "prettier": "^2.0.4",
    "prettier-miscellaneous": "^1.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.0.2"
  },
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@types/node": "^16.3.1",
    "antd": "^3.0.0-alpha0",
    "codemirror": "^5.61.0",
    "pegjs": "^0.10.0",
    "react-codemirror2": "^7.2.1",
    "svguitar": "^1.12.2"
  }
}
